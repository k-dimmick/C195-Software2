<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.addCustomerController">
   <children>
      <Button fx:id="cancelButton" layoutX="253.0" layoutY="586.0" mnemonicParsing="false" onAction="#cancelClick" text="Cancel" />
      <Button fx:id="saveButton" layoutX="135.0" layoutY="586.0" mnemonicParsing="false" onAction="#saveClick" text="Save" />
      <Label fx:id="customerName" layoutX="17.0" layoutY="125.0" text="Customer Name" />
      <Label fx:id="address" layoutX="17.0" layoutY="176.0" text="Address" />
      <Label fx:id="phoneNumber" layoutX="14.0" layoutY="231.0" text="Phone Number" />
      <Label fx:id="postalCode" layoutX="14.0" layoutY="286.0" text="Postal Code" />
      <TextField fx:id="customerNameInput" layoutX="162.0" layoutY="121.0" />
      <TextField fx:id="addressInput" layoutX="162.0" layoutY="172.0" />
      <TextField fx:id="phoneNumberInput" layoutX="162.0" layoutY="227.0" />
      <TextField fx:id="postalCodeInput" layoutX="162.0" layoutY="282.0" />
      <Button fx:id="appointmentsButton" layoutX="362.0" layoutY="586.0" mnemonicParsing="false" onAction="#appointmentsClick" text="Appointments" />
      <Label fx:id="customerID" layoutX="19.0" layoutY="84.0" text="Customer ID" />
      <TextField fx:id="customerIDInput" editable="false" layoutX="162.0" layoutY="80.0" promptText="System Generated" />
      <ComboBox fx:id="stateProvinceBox" layoutX="162.0" layoutY="359.0" onAction="#stateBoxClick" prefWidth="150.0" />
      <ComboBox fx:id="countryComboBox" layoutX="163.0" layoutY="437.0" onAction="#countryBoxClick" prefWidth="150.0" />
      <Label fx:id="stateProvinceLabel" layoutX="21.0" layoutY="363.0" text="State/Province" />
      <Label fx:id="countryLabel" layoutX="23.0" layoutY="441.0" text="Country" />
      <Button fx:id="customerButton" layoutX="501.0" layoutY="586.0" mnemonicParsing="false" onAction="#customerClick" text="Customers" />
   </children>
</AnchorPane>
