<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.allCustomerController">
   <children>
      <TableView fx:id="customerTable" layoutY="142.0" prefHeight="200.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="custIDCol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="custNameCol" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="custAddressCol" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="custPostCodeCol" prefWidth="75.0" text="Postal Code" />
            <TableColumn fx:id="custPhoneCol" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="custCreateDateCol" prefWidth="75.0" text="Create Date" />
            <TableColumn fx:id="custCreateByCol" prefWidth="75.0" text="Created By" />
            <TableColumn fx:id="custLastUpdateCol" prefWidth="75.0" text="Last Update" />
            <TableColumn fx:id="custLastUpdateByCol" prefWidth="114.0" text="Last Updated By" />
            <TableColumn fx:id="custDivisionIDCol" prefWidth="85.0" text="Division ID" />
        </columns>
      </TableView>
      <Button fx:id="deleteCustomerButton" layoutX="499.0" layoutY="388.0" mnemonicParsing="false" onAction="#deleteCustomerClick" text="Delete Customer" />
      <Button fx:id="modifyCustomerButton" layoutX="362.0" layoutY="388.0" mnemonicParsing="false" onAction="#modifyCustomerClick" text="Modify Customer" />
      <Button fx:id="addCustomerButton" layoutX="244.0" layoutY="388.0" mnemonicParsing="false" onAction="#addCustomerClick" text="Add Customer" />
      <Button fx:id="backToApptButton" layoutX="446.0" layoutY="466.0" mnemonicParsing="false" onAction="#backToClick" prefHeight="25.0" prefWidth="163.0" text="Back to Appointments" />
      <Button fx:id="reportsButton" layoutX="342.0" layoutY="466.0" mnemonicParsing="false" onAction="#reportsClick" text="View Reports" />
   </children>
</AnchorPane>
